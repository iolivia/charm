{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-console": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-shorthand": [
            "consistent"
        ],
        "eofline": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "object-literal-sort-keys": false
    }
}